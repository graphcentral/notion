    "notion-client": "^6.12.9",
    "serialize-error": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "esbuild-plugin-babel": "^0.2.3",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  }
